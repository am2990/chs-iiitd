<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 


<hibernate-mapping package="org.openmrs.module.basicexample">
    <class name="AmqpObs" table="amqp_obs">
        <id column="id" name="id" type="int">
            <generator class="native"/>
        </id>
        <property column="name" name="name" type="string" />
        <property column="uuid" name="uuid" type="string" />
        <property column="dob" name="dob" type="string" />
        <property column="gender" name="gender" type="string" />
        <property column="obs" name="obs" type="string" />
        <property column="created_date" name="created_date" type="string"/>
    </class>
</hibernate-mapping>